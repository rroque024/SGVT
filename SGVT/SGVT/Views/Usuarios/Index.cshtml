@model IEnumerable<SGVT.Models.Usuario>

@{
    ViewData["Title"] = "Index";
}

<section class="banner_area">
    <div class="banner_inner d-flex align-items-center">
        <div class="container">
            <div class="banner_content text-center">
                <h2>Shop Category Page</h2>
                <div class="page_link">
                    <a href="index.html">Home</a>
                    <a href="category.html">Category</a>
                    <a href="category.html">Women Fashion</a>
                </div>
            </div>
        </div>
    </div>
</section>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NuNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NuApaterno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NuAmaterno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NuCorreo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IuCelular)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NuDireccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NuContraseña)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkIdTipoUsuarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NuNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NuApaterno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NuAmaterno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NuCorreo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IuCelular)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NuDireccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NuContraseña)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkIdTipoUsuarioNavigation.PkIdTipoUsuario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PkDni">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.PkDni">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.PkDni">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
